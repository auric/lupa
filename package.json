{
    "name": "codelens-pr-analyzer",
    "displayName": "CodeLens PR Analyzer",
    "description": "A VS Code extension for comprehensive pull request analysis using GitHub Copilot models",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.91.0",
        "node": ">=20"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "extensionDependencies": [
        "vscode.git"
    ],
    "contributes": {
        "commands": [
            {
                "command": "codelens-pr-analyzer.analyzePR",
                "title": "CodeLens PR: Analyze Pull Request"
            },
            {
                "command": "codelens-pr-analyzer.selectEmbeddingModel",
                "title": "CodeLens PR: Select Embedding Model"
            },
            {
                "command": "codelens-pr-analyzer.showEmbeddingModelsInfo",
                "title": "CodeLens PR: Show Embedding Models Info"
            },
            {
                "command": "codelens-pr-analyzer.manageIndexing",
                "title": "CodeLens PR: Manage PR Analyzer Indexing"
            },
            {
                "command": "codelens-pr-analyzer.manageDatabase",
                "title": "CodeLens PR: Manage PR Analyzer Database"
            },
            {
                "command": "codelens-pr-analyzer.startContinuousIndexing",
                "title": "CodeLens PR: Start Continuous File Indexing"
            },
            {
                "command": "codelens-pr-analyzer.stopContinuousIndexing",
                "title": "CodeLens PR: Stop Continuous File Indexing"
            },
            {
                "command": "codelens-pr-analyzer.showLanguageModelsInfo",
                "title": "CodeLens PR: Show Language Models Info"
            },
            {
                "command": "codelens-pr-analyzer.selectLanguageModel",
                "title": "CodeLens PR: Select Language Model"
            },
            {
                "command": "codelens-pr-analyzer.testWebview",
                "title": "CodeLens PR: Test Webview (Development)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "build": "npm run check-types && npm run build:node && npm run build:webview",
        "build:node": "vite build --mode development",
        "build:webview": "cross-env BUILD_TARGET=webview vite build --mode development",
        "watch": "npm run check-types && npm run build:node && npm run build:webview",
        "package": "npm run clean && npm run check-types && npm run package:node && npm run package:webview",
        "package:node": "vite build --mode production",
        "package:webview": "cross-env BUILD_TARGET=webview vite build --mode production",
        "pretest": "npm run build",
        "prepare-models": "node ./scripts/prepare-models.js",
        "check-types": "tsc --noEmit",
        "clean": "rimraf -g ./*.vsix && rimraf ./dist",
        "test": "npx vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@huggingface/transformers": "^3.5.2",
        "@radix-ui/react-accordion": "^1.2.11",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.12",
        "@tailwindcss/postcss": "^4.1.11",
        "@vscode/sqlite3": "^5.1.8-vscode",
        "@vscode/tree-sitter-wasm": "^0.1.4",
        "async-mutex": "^0.5.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "hnswlib-node": "^3.0.0",
        "lucide-react": "^0.525.0",
        "ml-distance": "^4.0.1",
        "onnxruntime-node": "^1.21.0",
        "react": "^19.1.0",
        "react-diff-view": "^3.3.1",
        "react-dom": "^19.1.0",
        "react-markdown": "^10.1.0",
        "react-syntax-highlighter": "^15.6.1",
        "rehype-highlight": "^7.0.2",
        "remark-gfm": "^4.0.1",
        "sql-template-strings": "^2.2.2",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "tinypool": "^1.1.0",
        "uuid": "^11.1.0",
        "web-tree-sitter": "^0.25.5"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "@types/node": "^22.13.8",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.97.0",
        "@vitejs/plugin-react": "^4.6.0",
        "@vitest/coverage-v8": "^3.2.3",
        "babel-plugin-react-compiler": "^19.1.0-rc.2",
        "cross-env": "^7.0.3",
        "rimraf": "^6.0.1",
        "tw-animate-css": "^1.3.5",
        "typescript": "^5.8.2",
        "vite": "^7.0.4",
        "vite-plugin-static-copy": "^3.1.1",
        "vitest": "^3.2.3"
    },
    "files": [
        "dist/**/*"
    ]
}
